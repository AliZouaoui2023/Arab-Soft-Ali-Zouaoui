<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<xs:complexType name="T_Entete">
		<xs:annotation>
			<xs:documentation>En-tête de la déclaration des comptes dormants</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MatriculeFiscal" type="T_MatriculeFiscal" minOccurs="1"/>
			<xs:element name="RaisonSocial" type="T_ChaineTexte250" minOccurs="1"/>
			<xs:element name="Exercice" type="T_Annee" minOccurs="1"/>
			<xs:element name="ActeDeDepot" type="T_ActeDeDepot" minOccurs="1"/>
			<xs:element name="ReferenceDeclaration" type="T_RefDec" minOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="T_VersionDoc">
		<xs:annotation>
			<xs:documentation>
		  Ce type représente la version du document, fixée à "version-1.0" conformément à la convention de versionnement sémantique.
		</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="version-1.0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="T_MatriculeFiscal">
		<xs:annotation>
			<xs:documentation>
				Référence alphanumérique : 7 chiffres (≠ 0000000) suivis d’une lettre majuscule (A-Z),
				Exemple valide : 1234567A, 0000001Z, etc.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="^[1-9][0-9]{6}[A-HJ-NP-Z]$"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="T_ChaineTexte250">
		<xs:annotation>
			<xs:documentation>
		  Chaîne de texte de 250 caractères maximum, uniquement lettres (accentuées ou non) et espaces. Chiffres et caractères spéciaux interdits.
		</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="250"/>
			<xs:whiteSpace value="preserve"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="T_Annee">
		<xs:annotation>
			<xs:documentation>
				Exercice au titre duquel les comptes, valeurs mobilières, contrats d’assurance, etc., seront déclarés.
				L'exercice doit être ≥ 2024.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:gYear">
			<xs:minInclusive value="2024"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="T_ActeDeDepot">
		<xs:annotation>
			<xs:documentation>Actes de dépôts</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="1"/>
			<xs:enumeration value="0"/>
			<!-- declaration spontannée (initiale)  -->
			<xs:enumeration value="1"/>
			<!-- declaration rectificative -->
			<xs:enumeration value="2"/>
			<!-- declaration déposée suite intervention des services de contrôle  -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="T_RefDec">
		<xs:annotation>
			<xs:documentation>
				Référence de décision : entier positif (max 7 chiffres), différent de 0
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="9999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="T_TypeAction">
		<xs:annotation>
			<xs:documentation>Type de l'action de dépôt : A (ajout), M (modification), I (inchangé).
                           Seules les majuscules sont autorisées.
        </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:length value="1"/>
			<xs:enumeration value="A"/>
			<xs:enumeration value="M"/>
			<xs:enumeration value="I"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="T_TypeDevise">
		<xs:restriction base="xs:string">
			<xs:enumeration value="788"/>
			<!-- TND = Dinar Tunisien -->
			<xs:enumeration value="978"/>
			<!-- EUR = Euro -->
			<xs:enumeration value="840"/>
			<!-- USD = Dollar des États-Unis -->
			<xs:enumeration value="826"/>
			<!-- GBP = Livre Sterling -->
			<xs:enumeration value="392"/>
			<!-- JPY = Yen Japonais -->
			<xs:enumeration value="756"/>
			<!-- CHF = Franc Suisse -->
			<xs:enumeration value="124"/>
			<!-- CAD = Dollar Canadien -->
			<xs:enumeration value="414"/>
			<!-- KWD = Dinar Koweïtien -->
			<xs:enumeration value="682"/>
			<!-- SAR = Riyal Saoudien -->
			<xs:enumeration value="784"/>
			<!-- AED = Dirham EAU -->
			<xs:enumeration value="634"/>
			<!-- QAR = Riyal Qatari -->
			<xs:enumeration value="048"/>
			<!-- BHD = Dinar Bahreïni -->
			<xs:enumeration value="156"/>
			<!-- CNY = Yuan Renminbi Chinois -->
			<xs:enumeration value="434"/>
			<!-- LYD = Dinar Libyen -->
			<xs:enumeration value="752"/>
			<!-- SEK = Couronne suédoise -->
			<xs:enumeration value="208"/>
			<!-- DKK = Couronne danoise -->
			<xs:enumeration value="578"/>
			<!-- NOK = Couronne norvégienne -->
			<xs:enumeration value="008"/>
			<!-- ALL = Lek Albanais -->
			<xs:enumeration value="036"/>
			<!-- AUD = Dollar Australien -->
			<xs:enumeration value="040"/>
			<!-- ATS = Schilling Autrichien -->
			<xs:enumeration value="056"/>
			<!-- BEF = Franc Belge -->
			<xs:enumeration value="220"/>
			<!-- SCO = Livre Écossaise (non officiel) -->
			<xs:enumeration value="246"/>
			<!-- FIM = Mark Finlandais -->
			<xs:enumeration value="250"/>
			<!-- FRF = Franc Français -->
			<xs:enumeration value="280"/>
			<!-- DEM = Deutsche Mark -->
			<xs:enumeration value="300"/>
			<!-- GRD = Drachme Grecque -->
			<xs:enumeration value="328"/>
			<!-- GYD = Dollar Guyanais -->
			<xs:enumeration value="356"/>
			<!-- INR = Roupie Indienne -->
			<xs:enumeration value="372"/>
			<!-- IEP = Livre Irlandaise -->
			<xs:enumeration value="380"/>
			<!-- ITL = Lire Italienne -->
			<xs:enumeration value="410"/>
			<!-- KRW = Won Sud-Coréen -->
			<xs:enumeration value="422"/>
